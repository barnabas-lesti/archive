    "codelyzer": "^6.0.0",
    "jest": "^26.6.3",
    "jest-html-reporters": "^2.1.2",
    "jest-preset-angular": "^8.3.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
