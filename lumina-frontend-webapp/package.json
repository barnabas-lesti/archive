  "description": "Lumina frontend web application",
  "author": "Barnabas Lesti <barnabas.lesti@gmail.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start:dev:watch": "ng serve",
    "start:prod": "npm run build:prod && angular-http-server -p 3000 --path dist/lumina-frontend-webapp",
    "build:dev": "ng build",
    "build:prod": "ng build --prod",
    "test": "npm run test:specs && npm run test:mutation",
    "test:specs": "jest --config jest.config.specs.js",
    "test:mutation": "stryker run",
    "lint": "ng lint"
  },
  "dependencies": {
    "@angular/animations": "~11.0.6",
    "@angular/cdk": "^11.1.1",
    "@angular/common": "~11.0.6",
    "@angular/compiler": "~11.0.6",
    "@angular/core": "~11.0.6",
    "@angular/forms": "~11.0.6",
    "@angular/platform-browser": "~11.0.6",
    "@angular/platform-browser-dynamic": "~11.0.6",
    "@angular/router": "~11.0.6",
    "@fortawesome/fontawesome-free": "^5.15.2",
    "angular-http-server": "^1.9.0",
    "moment": "^2.29.1",
    "primeflex": "^2.0.0",
    "primeicons": "^4.1.0",
    "primeng": "^11.2.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.6",
    "@angular/cli": "~11.0.6",
